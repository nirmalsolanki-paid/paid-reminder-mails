{
  "name": "api.paid.com",
  "version": "1.0.0",
  "type": "module",
  "main": "src/app-server.ts",
  "scripts": {
    "start": "npm run build && ts-node dist/app-server.js",
    "build": "tsup --config tsup.config.ts && node -e \"fs.cpSync('src/images','dist/images',{recursive:true})\""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "npm run lint:prettier",
      "npm run eslint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "keywords": [],
  "author": "Nirmal Solanki <nirmal@shiptime.com>",
  "license": "ISC",
  "description": "The Paid Inc. services api.",
  "dependencies": {
    "@google-cloud/secret-manager": "^6.1.1",
    "@sendgrid/mail": "^8.1.4",
    "express": "^4.21.2",
    "express-winston": "^4.2.0",
    "moment": "^2.30.1",
    "mongoose": "^7.8.8",
    "morgan": "^1.10.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
